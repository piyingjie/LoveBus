<?xml version="1.0" encoding="utf-8"?>
<resources>
<declare-styleable name="RaidoTextView">
    <attr name="titleText" format="string" />
    <attr name="backgrounds" format="color"/>
    <attr name="titleTextSize" format="dimension"/>
    <attr name="titleTextColor" format="color"/>
    <attr name="mCornerSize" format="integer"/>
    <attr name="gravity" format="enum">
        <enum name="center" value="0"/>
        <enum name="top" value="1"/>
        <enum name="bottom" value="2"/>
        <enum name="left" value="3"/>
        <enum name="right" value="4"/>
        <enum name="center_horizontal" value="5"/>
        <enum name="center_vertical" value="6"/>
    </attr>
</declare-styleable>
</resources>