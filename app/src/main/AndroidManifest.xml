<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bus.android.com.lovebus">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        >
        <activity android:name="com.lovebus.activity.Main_Activity"
                  android:theme="@style/AppTheme"/>
        <activity android:name="com.lovebus.activity.StartPictureActivity"
                  android:theme="@style/FullAppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lovebus.activity.FirstLoginActivity"
                  android:theme="@style/AppTheme"/>
        <activity android:name="com.lovebus.activity.SelectCityActivity" />
        <activity android:name="com.lovebus.activity.OffLineMapActivity" />
        <activity android:name="com.lovebus.activity.FrequentRouteActivity" />
        <activity android:name="com.lovebus.activity.UserCollectionActivity" />
        <activity android:name="com.lovebus.activity.RemindSettingsActivity" />
        <activity android:name="com.lovebus.activity.NoticeActivity" />
        <activity android:name="com.lovebus.activity.SystemSettingsActivity" />
        <activity android:name="com.lovebus.activity.PrivacyAndSafetyActivity" />
        <activity android:name="com.lovebus.activity.ThirdPartyBindActivity"></activity>
    </application>

</manifest>